{
    "api_key": {
        "description": "阿里云通义万像API密钥",
        "type": "string",
        "hint": "请填写API密钥",
        "default": "",
        "obvious_hint": true
    },
    "model": {
        "description": "通义万像模型选择",
        "type": "string",
        "hint": "选择要使用的模型",
        "default": "wanx2.1-t2i-turbo",
        "options": ["wanx2.1-t2i-turbo", "wanx2.1-t2v-turbo", "wanx2.1-i2v-turbo"]
    },
    "trigger_words": {
        "description": "触发词配置",
        "type": "object",
        "hint": "设置不同功能的触发词",
        "items": {
            "image_keywords": {
                "description": "文生图触发词",
                "type": "string",
                "hint": "多个关键词用逗号分隔",
                "default": "画图,绘画,生成图片"
            },
            "video_keywords": {
                "description": "文生视频触发词",
                "type": "string",
                "hint": "多个关键词用逗号分隔",
                "default": "生成视频,视频生成"
            },
            "i2v_keywords": {
                "description": "图生视频触发词",
                "type": "string",
                "hint": "多个关键词用逗号分隔",
                "default": "图片转视频,转视频"
            }
        }
    }
}